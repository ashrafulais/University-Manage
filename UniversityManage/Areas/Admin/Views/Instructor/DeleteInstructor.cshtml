@model UniversityManage.Model.Student;

@{
    ViewData["Title"] = "Delete Student";
}


@if (Model != null)
{
    <h3>Delete student: @Model.Id - @Model.Name</h3>
    <a href="/admin/student/ConfirmDeleteStudent/@Model.Id" class="btn btn-outline-warning">Confirm</a>
    <a href="AllStudents" class="btn btn-outline-warning">Don't</a>
}

