@model UniversityManage.Model.Instructor;

@{
    ViewData["Title"] = "Delete Instructor";
}


@if (Model != null)
{
    <h3>Delete student: @Model.Id - @Model.Name</h3>
    <a href="/admin/Instructor/ConfirmDeleteInstructor/@Model.Id" class="btn btn-outline-warning">Confirm</a>
    <a href="AllInstructors" class="btn btn-outline-warning">Don't</a>
}

